@model Samochod

<h2 style="color: white">@Model.Marka - @Model.Model</h2>

<div class="img-thumbnail">
    <img class="image" src="~/@Model.MiniaturkaUrl" alt="@Model.Marka" />
    <div class="figure-caption">
        <h3 class="float-right">@Model.Cena</h3>
        <h4>@Model.RokProdukcji</h4>
        <h4>@Model.Przebieg</h4>
        <h4>@Model.Pojemnosc</h4>
        <h4>@Model.Moc</h4>
        <p>@Model.Opis</p>
    </div>
</div>